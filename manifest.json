{
    "manifest_version": 3,
    "name": "Tab Teleporter",
    "version": "1.0",
    "description": "Instantly transfer and open all synced tabs from your Chrome devices at once.",
    "permissions": [
      "sessions",
      "tabs"
    ],
    "action": {
      "default_title": "Tab Teleporter",
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icons/Icon_Tab_Teleporter_large_16.png",
        "48": "icons/Icon_Tab_Teleporter_large_48.png",
        "128": "icons/Icon_Tab_Teleporter_large_128.png"
      }
    },
    "icons": {
      "128": "icons/Icon_Tab_Teleporter_large_128.png"
    }
  }
  